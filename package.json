    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "./.circleci/config.yml": [
            "yarn lint:ci",
            "git add"
        ],
        "*.{js,jsx}": [
            "yarn lint:fix:js",
            "git add"
        ],
        "*.{ts,tsx}": [
            "yarn lint:fix:ts",
            "yarn lint:style",
            "git add"
        ],
        "*.{json}": [
            "yarn lint:fix:json",
            "git add"
        ]
