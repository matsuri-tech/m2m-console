        "build-dev": "webpack --mode development",
        "start": "npx serve dist",
        "test": "jest",
        "update": "yarn upgrade-interactive --latest",
        "lint:ts": "eslint ./src/**/*.ts ./src/**/*.tsx  --cache",
        "lint:js": "eslint ./*.js --cache",
